{
    "name": "marshmallow/breadcrumb",
    "description": "A package for Laravel Breadcrum usage with the ability to add some default and use your custom templating",
    "keywords": [
        "marshmallow"
    ],
    "homepage": "https://github.com/Marshmallow-Development/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stef",
            "email": "stef@marshmallow.dev",
            "homepage": "https://marshmallow.dev",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "stef@marshmallow.dev"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0|^8.0",
        "marshmallow/commands": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\Breadcrumb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marshmallow\\Breadcrumb\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\Breadcrumb\\BreadcrumbServiceProvider"
            ],
            "aliases": {
                "Breadcrumb": "Marshmallow\\Breadcrumb\\Facades\\Breadcrumb"
            }
        }
    }
}
